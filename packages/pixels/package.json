{
  "name": "@fuf-stack/pixels",
  "description": "fuf react component library",
  "author": "Hannes Tiede",
  "version": "0.3.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./Button": {
      "import": "./dist/Button/index.js",
      "require": "./dist/Button/index.cjs",
      "types": "./dist/Button/index.d.ts"
    },
    "./Card": {
      "import": "./dist/Card/index.js",
      "require": "./dist/Card/index.cjs",
      "types": "./dist/Card/index.d.ts"
    },
    "./Json": {
      "import": "./dist/Json/index.js",
      "require": "./dist/Json/index.cjs",
      "types": "./dist/Json/index.d.ts"
    },
    "./Label": {
      "import": "./dist/Label/index.js",
      "require": "./dist/Label/index.cjs",
      "types": "./dist/Label/index.d.ts"
    },
    "./Menu": {
      "import": "./dist/Menu/index.js",
      "require": "./dist/Menu/index.cjs",
      "types": "./dist/Menu/index.d.ts"
    },
    "./Modal": {
      "import": "./dist/Modal/index.js",
      "require": "./dist/Modal/index.cjs",
      "types": "./dist/Modal/index.d.ts"
    },
    "./Popover": {
      "import": "./dist/Popover/index.js",
      "require": "./dist/Popover/index.cjs",
      "types": "./dist/Popover/index.d.ts"
    },
    "./Tooltip": {
      "import": "./dist/Tooltip/index.js",
      "require": "./dist/Tooltip/index.cjs",
      "types": "./dist/Tooltip/index.d.ts"
    }
  },
  "files": [
    "./dist"
  ],
  "homepage": "https://github.com/fuf-stack/uniform/tree/main/packages/pixels",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "components",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuf-stack/uniforms.git",
    "directory": "packages/pixels"
  },
  "bugs": {
    "url": "https://github.com/fuf-stack/uniforms/issues"
  },
  "scripts": {
    "build": "tsup --config node_modules/@repo/tsup-config/config.ts",
    "prepack": "rm -rf ./dist && pnpm build",
    "test": "vitest ./src"
  },
  "dependencies": {
    "@nextui-org/button": "2.0.27",
    "@nextui-org/card": "2.0.24",
    "@nextui-org/chip": "2.0.25",
    "@nextui-org/divider": "2.0.25",
    "@nextui-org/dropdown": "2.1.16",
    "@nextui-org/modal": "2.0.29",
    "@nextui-org/popover": "2.1.15",
    "@nextui-org/system": "2.0.15",
    "@nextui-org/tooltip": "2.0.30",
    "classnames": "2.5.1",
    "debug": "4.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-json-view": "1.21.3",
    "@uiw/react-json-view": "2.0.0-alpha.24",
    "tailwind-variants": "0.1.20"
  },
  "devDependencies": {
    "@repo/storybook-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/tsup-config": "workspace:*",
    "@repo/vite-config": "workspace:*",
    "@types/debug": "4.1.12",
    "@types/react": "18.2.69",
    "@types/react-dom": "18.2.22"
  }
}
