{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@fuf-stack/typescript-config/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "paths": {
      "src/*": ["./src/*"],
      "test/*": ["./test/*"]
    },
    "rootDir": ".",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
  },
  "include": ["src", "test"]
}
